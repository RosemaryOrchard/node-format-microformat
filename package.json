    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.0.0",
    "eslint-plugin-standard": "^3.0.1",
    "husky": "^0.13.3",
    "installed-check": "^2.0.0",
    "istanbul": "^0.4.5",
    "mocha": "^3.1.2",
    "sinon": "^2.2.0",
    "sinon-chai": "^2.10.0"
  },
